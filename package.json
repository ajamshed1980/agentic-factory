{
  "name": "agentic-factory",
  "private": true,
  "scripts": {
    "lint": "eslint apps/web --ext .ts,.tsx,.js,.jsx --no-error-on-unmatched-pattern",
    "lint:fix": "eslint apps/web --ext .ts,.tsx,.js,.jsx --fix --no-error-on-unmatched-pattern",
    "ci:web": "npm ci --prefix apps/web && npm --prefix apps/web run test -- --ci && npm --prefix apps/web run test:e2e:install && npm --prefix apps/web run test:e2e"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "prettier": "^3.6.2",
    "typescript-eslint": "^8.44.0"
  }
}
